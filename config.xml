<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.ionichelloworld" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Ionic Hello World</name>
  <description>An Ionic Framework and Cordova project.</description>
  <author email="dedycated.inc@gmail.com" href="http://dedycated.com/">DedyCated Labs</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="geo:*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="market:*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <icon src="www/img/ionic.png"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="SplashScreenDelay" value="2000"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="cordova-plugin-contacts" spec="~2.2.0"/>
  <plugin name="cordova-plugin-geolocation" spec="~2.3.0"/>
  <plugin name="cordova-plugin-camera" spec="~2.3.0"/>
  <plugin name="cordova-plugin-console" spec="~1.0.4"/>
  <plugin name="cordova-plugin-device" spec="~1.1.3"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.2.0"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.0"/>
  <plugin name="cordova.plugins.diagnostic" spec="~3.3.2"/>
  <plugin name="plugin.google.maps" spec="~1.3.9">
    <variable name="API_KEY_FOR_ANDROID" value="AIzaSyBEWr-HZ4sQZWuOLjaS4kQeqrbG8fIQMu4"/>
    <variable name="API_KEY_FOR_IOS" value="AIzaSyACyr7bPn4deawGZHO0OM7liRPRbj0zd0A"/>
  </plugin>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="phonegap-plugin-barcodescanner" spec="~6.0.3"/>
</widget>
